'No. 430
Private Sub Command1_Click()
Dim x() As Integer
m = Val(InputBox("M: "))
Pic1.Scale (0, 0)-(m + 1, m + 1)
ReDim x(1 To m, 1 To m) As Integer
Pic1.Cls
For i = 1 To m
For j = 1 To m
x(i, j) = Rnd * 100
Pic1.CurrentX = j
Pic1.CurrentY = i
Pic1.Print x(i, j)
If i >= j And x(i, j) Mod 2 = 0 Then
q = q + 1
End If
Next
Next
If q <> 0 Then
MsgBox "Quanity: " & q
Else
MsgBox "Quanity: 0"
End If
End Sub
